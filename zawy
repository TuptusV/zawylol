-- DA MILITARY FPS BOOST SCRIPT
-- Wklej jako LocalScript

-- LocalScript w StarterPlayerScripts

local StarterGui = game:GetService("StarterGui")

-- Funkcja do wysyłania powiadomień
local function sendNotification(title, text, duration)
    StarterGui:SetCore("SendNotification", {
        Title = title or "Notification";
        Text = text or "";
        Duration = duration or 3; -- czas w sekundach
        Button1 = "okay"; -- opcjonalny przycisk
    })
end

-- Przykład użycia:
sendNotification("ZAWY ZAWY ZAWY", "Your FPS Boost is active", 5)


local Lighting = game:GetService("Lighting")
local Terrain = workspace:FindFirstChildOfClass("Terrain")

-- Najniższa jakość grafiki
settings().Rendering.QualityLevel = Enum.QualityLevel.Level01

-- Wyłączenie cieni i refleksów
Lighting.GlobalShadows = false
Lighting.EnvironmentDiffuseScale = 0
Lighting.EnvironmentSpecularScale = 0
Lighting.Brightness = 1
Lighting.FogEnd = 9e9
Lighting.ClockTime = 14

-- Usunięcie post process effectów
for _, effect in ipairs(Lighting:GetChildren()) do
	if effect:IsA("PostEffect") then
		effect:Destroy()
	end
end

-- Obniżenie jakości wody (jeśli mapa jej używa)
if Terrain then
	Terrain.WaterWaveSize = 0
	Terrain.WaterWaveSpeed = 0
	Terrain.WaterReflectance = 0
	Terrain.WaterTransparency = 1
end

-- Usuwanie tekstur z terenu i obiektów
for _, obj in ipairs(workspace:GetDescendants()) do
	if obj:IsA("Decal") or obj:IsA("Texture") then
		obj:Destroy()
	end
end

-- FUNKCJA: uproszczenie części gry
local function optimize(obj)
	-- Wygaszenie cieni i detalu z części
	if obj:IsA("BasePart") then
		obj.Material = Enum.Material.Plastic
		obj.CastShadow = false
		obj.Reflectance = 0
	end

	-- Usunięcie particle efektów (wybuchy, dym, ogień)
	if obj:IsA("ParticleEmitter") or obj:IsA("Fire") or obj:IsA("Smoke") then
		obj.Enabled = false
	end

	-- Zmniejszenie świateł
	if obj:IsA("PointLight") or obj:IsA("SpotLight") or obj:IsA("SurfaceLight") then
		obj.Enabled = false
	end
end

-- Optymalizacja wszystkich istniejących obiektów
for _, v in ipairs(workspace:GetDescendants()) do
	optimize(v)
end

-- Optymalizacja nowych obiektów (np. wybuchów w trakcie gry)
workspace.DescendantAdded:Connect(function(obj)
	optimize(obj)
end)

-- Ustawienia FPS Clienta
local RunService = game:GetService("RunService")

RunService:Set3dRenderingEnabled(true) -- musi być true, inaczej error

-- Usuwanie gun flash i recoil (jeśli gra używa efektów clienta)
-- Działa tylko jeśli efekty są client-side
local function removeGunEffects()
	for _, obj in ipairs(workspace:GetDescendants()) do
		if obj.Name:lower():find("flash") or
		   obj.Name:lower():find("smoke") or
		   obj.Name:lower():find("muzzle") then
			obj:Destroy()
		end
	end
end

removeGunEffects()

workspace.DescendantAdded:Connect(function(obj)
	if obj.Name:lower():find("flash") or
	   obj.Name:lower():find("smoke") or
	   obj.Name:lower():find("muzzle") then
		obj:Destroy()
	end
end)

print("zawyW")

-- DA MILITARY ULTRA FPS BOOST SCRIPT
-- LocalScript w StarterPlayerScripts

local StarterGui = game:GetService("StarterGui")
local Lighting = game:GetService("Lighting")
local Workspace = game:GetService("Workspace")
local RunService = game:GetService("RunService")
local Terrain = Workspace:FindFirstChildOfClass("Terrain")

-- Funkcja powiadomień
local function sendNotification(title, text, duration)
    StarterGui:SetCore("SendNotification", {
        Title = title or "Notification";
        Text = text or "";
        Duration = duration or 3;
        Button1 = "OK";
    })
end

-- Ustawienia grafiki minimalnej
settings().Rendering.QualityLevel = Enum.QualityLevel.Level01
Lighting.GlobalShadows = false
Lighting.EnvironmentDiffuseScale = 0
Lighting.EnvironmentSpecularScale = 0
Lighting.Brightness = 1
Lighting.FogEnd = 9e9
Lighting.ClockTime = 14

-- Usunięcie post-process efektów
for _, effect in ipairs(Lighting:GetChildren()) do
    if effect:IsA("PostEffect") then
        effect:Destroy()
    end
end

-- Optymalizacja terenu
if Terrain then
    Terrain.WaterWaveSize = 0
    Terrain.WaterWaveSpeed = 0
    Terrain.WaterReflectance = 0
    Terrain.WaterTransparency = 1
end

-- Funkcja optymalizacji obiektów
local function optimize(obj)
    if obj:IsA("BasePart") then
        obj.Material = Enum.Material.Plastic
        obj.CastShadow = false
        obj.Reflectance = 0
        obj.TextureID = "" -- usuwa dodatkowe tekstury
    elseif obj:IsA("MeshPart") then
        obj.Material = Enum.Material.Plastic
        obj.CastShadow = false
        obj.Reflectance = 0
    elseif obj:IsA("Decal") or obj:IsA("Texture") then
        obj:Destroy()
    elseif obj:IsA("ParticleEmitter") or obj:IsA("Fire") or obj:IsA("Smoke") then
        obj.Enabled = false
    elseif obj:IsA("PointLight") or obj:IsA("SpotLight") or obj:IsA("SurfaceLight") then
        obj.Enabled = false
    end
end

-- Optymalizacja wszystkich istniejących obiektów
for _, v in ipairs(Workspace:GetDescendants()) do
    optimize(v)
end

-- Optymalizacja nowych obiektów
Workspace.DescendantAdded:Connect(optimize)

-- Usuwanie efektów broni client-side
local function removeGunEffects(obj)
    if obj.Name:lower():find("flash") or
       obj.Name:lower():find("smoke") or
       obj.Name:lower():find("muzzle") then
        obj:Destroy()
    end
end

for _, v in ipairs(Workspace:GetDescendants()) do
    removeGunEffects(v)
end

Workspace.DescendantAdded:Connect(removeGunEffects)

-- Wymuszenie 3D Rendering, aby uniknąć błędów
RunService:Set3dRenderingEnabled(true)

-- Odciążenie skryptów i eventów
pcall(function()
    Workspace:SetAttribute("OptimizeFPS", true)
end)
